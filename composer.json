{
    "name": "tholcomb/symple",
    "description": "Yet Another Symfony-based PHP Framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Holcomb",
            "email": "tyler@tholcomb.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Tholcomb\\Symple\\": "src/"
        },
        "files": [
            "src/Core/functions.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "doctrine/annotations": "^1.8",
        "doctrine/cache": "^1.10",
        "monolog/monolog": "^2.0",
        "pimple/pimple": "^3.2",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/error-handler": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/twig-bridge": "^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "replace": {
        "tholcomb/symple-core": "self.version",
        "tholcomb/symple-event": "self.version",
        "tholcomb/symple-http": "self.version",
        "tholcomb/symple-logger": "self.version",
        "tholcomb/symple-twig": "self.version"
    }
}
